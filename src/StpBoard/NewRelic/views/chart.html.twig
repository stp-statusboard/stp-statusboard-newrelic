<style>
    .newrelic > .chart {
        width: 100%;
        height: 100%;
    }
</style>
<div class="boardContent newrelic">
    <div id="chart-{{ name }}" class="chart"></div>
</div>
<script src="/js/highcharts-style.js"></script>
<script type="text/javascript">
    $('document').ready(function () {
        new Highcharts.Chart({
            chart: {
                renderTo: 'chart-{{ name }}',
                type: 'area'
            },
            title: {
                text: '{{ name }}'
            },
            xAxis: {
                title: '',
                type: 'datetime',
                tickPixelInterval: 150
            },
            yAxis: {
                title: ''
            },
            plotOptions: {
                area: {
                    pointStart: 1940,
                    marker: {
                        enabled: false,
                        symbol: 'circle',
                        radius: 2,
                        states: {
                            hover: {
                                enabled: true
                            }
                        }
                    }
                }
            },
            legend: {
                enabled: false
            },
            exporting: {
                enabled: false
            },
            series: [{
                data: JSON.parse('{{ data|json_encode|raw }}')
            }]
        });
    });
</script>
